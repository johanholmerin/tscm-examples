    "@babel/traverse": "^7.15.4",
    "@babel/types": "^7.15.6",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@pgtyped/cli": "^0.12.0",
    "@pgtyped/query": "^0.12.0",
    "ajv": "^8.6.3",
    "babel-plugin-macros": "^3.1.0",
    "do-sync": "^3.0.12",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.1",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^10.1.5",
    "pg": "^8.7.1",
    "ts-graphql-plugin": "^2.1.4",
    "tscm": "git+https://github.com/johanholmerin/tscm#master",
    "typecheck.macro": "^0.7.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.4.16",
    "@types/pg": "^8.6.1",
    "@types/react": "^17.0.27",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^27.2.1",
    "jest-file-snapshot": "^0.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.0",
    "tscm-examples": "link:.",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "tscm",
    "test": "jest",
    "lint": "eslint '**/*.js' --fix",
    "lint:check": "eslint '**/*.js'",
    "format": "prettier '**/*.{js,ts}' --write",
    "format:check": "prettier '**/*.{js,ts}' --check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run format"
    ],
    "*.ts": [
      "npm run format"
    ]
  }
}
